<merge xmlns:android="http://schemas.android.com/apk/res/android">
    <LinearLayout style="@style/OptionLeft">
        <TextView
            style="@style/OptionTitle"
            android:text="Auto Fast Detection" />
        <TextView
            style="@style/OptionDesc"
            android:text="Fast detection mode for streaming faces detection." />
        <RadioGroup
            android:id="@+id/radioFastDetection"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp">
            <RadioButton
                android:id="@+id/radioFastDetectionOff"
                style="@style/OptionValue"
                android:tag="0"
                android:text="Normal"/>
            <RadioButton
                android:id="@+id/radioFastDetectionAuto"
                style="@style/OptionValue"
                android:checked="true"
                android:tag="1"
                android:text="Fast"/>
        </RadioGroup>
    </LinearLayout>
</merge>