<merge xmlns:android="http://schemas.android.com/apk/res/android">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <Switch
            android:id="@+id/btnLandmark"
            style="@style/CLSwitch"/>
        <LinearLayout
            style="@style/OptionLeft"
            android:layout_toLeftOf="@id/btnLandmark">
            <TextView
                style="@style/OptionTitle"
                android:text="Landmark" />
            <TextView
                style="@style/OptionDesc"
                android:text="Show coordinates of eyes, nose and mouth" />
        </LinearLayout>
    </RelativeLayout>
</merge>